{
  "name": "ts-webpack-starter",
  "version": "1.0.0",
  "author": "markfknight (mark@markfknight.com)",
  "license": "MIT",
  "scripts": {
    "start": "npm run webpack:dev-server",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "build": "npm run webpack",
    "webpack": "webpack --progress --config webpack.prod.js",
    "webpack:watch": "webpack --progress --watch --config webpack.prod.js",
    "webpack:dev-server": "webpack-dev-server --open --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markfknight/ts-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/markfknight/ts-webpack-starter/issues"
  },
  "homepage": "https://github.com/markfknight/ts-webpack-starter#readme",
  "devDependencies": {
    "@types/jest": "^21.0.0",
    "@types/node": "^8.0.31",
    "@types/webpack-env": "^1.13.1",
    "awesome-typescript-loader": "^3.2.3",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "notify": true,
    "rootDir": "src/",
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
